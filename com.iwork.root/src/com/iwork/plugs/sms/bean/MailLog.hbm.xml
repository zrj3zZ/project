<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.iwork.plugs.sms.bean">
	<class table="IWORK_MAIL_LOG" name="MailLog">
		<id name="cid" column="CID" type="integer">
			<generator class="increment"></generator>
		</id>
		<property name="userid" type="java.lang.String" column="USERID">
		</property>
		<property name="content" type="java.lang.String" column="CONTENT">
		</property>
		<property name="title" type="java.lang.String" column="TITLE">
		</property>
		<property name="email" type="java.lang.String" column="EMAIL">
		</property>
		<property name="attach" type="java.lang.String" column="ATTACH">
		</property>
		<property name="submittime" type="java.util.Date" column="SUBMITTIME">
		</property>
		<property name="status" type="integer" column="STATUS">
		</property>
		<property name="sendtime" type="java.util.Date" column="SENDTIME">
		</property>
		<property name="name" type="java.lang.String" column="NAME">
		</property>
	</class>
</hibernate-mapping>
