<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.iwork.plugs.sms.bean">
    <class table="IWORK_SMS_CHANNEL" name="ChannelMst">
		<id name="cid" column="cid" type="integer">
			<generator class="increment"></generator>
		</id>
            <property name="channelid" type="integer" column="CHANNELID"></property>
            <property name="description" type="java.lang.String" column="DESCRIPTION"></property>
            <property name="msgsp" type="integer" column="MSGSP"></property>
       
    </class>
</hibernate-mapping>
