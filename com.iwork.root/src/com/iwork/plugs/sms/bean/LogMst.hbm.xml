<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.iwork.plugs.sms.bean">
	
	
	<class table="IWORK_SMS_LOG" name="LogMst">
		<id name="cid" column="cid" type="integer">
			<generator class="increment"></generator>
		</id>
		
            <property name="userid" type="java.lang.String" column="USERID" >
            </property>
            <property name="logtime" type="java.util.Date" column="LOGTIME">
            </property>
            <property name="logtype" type="java.lang.String" column="LOGTYPE" >
            </property>
            <property name="value" type="java.lang.String" column="VALUE">
            </property>

    </class>
</hibernate-mapping>
