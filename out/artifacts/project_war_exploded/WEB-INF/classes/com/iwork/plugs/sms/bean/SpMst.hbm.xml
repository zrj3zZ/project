<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.iwork.plugs.sms.bean">
	
	
	<class table="IWORK_SMS_SP" name="SpMst">
		<id name="cid" column="cid" type="integer">
			<generator class="increment"></generator>
		</id>
		
            <property name="spid" type="integer" column="SPID">
            </property>
            <property name="name" type="java.lang.String" column="NAME">
            </property>
            <property name="description" type="java.lang.String" column="DESCRIPTION">
            </property>
            <property name="sign" type="java.lang.String"  column="SIGN" >
            </property>
            <property name="billinglen" type="integer" column="BILLINGLEN">
            </property>
            <property name="price" type="java.lang.Double" column="PRICE">
            </property>
            <property name="suppsplit" type="integer" column="SUPPSPLIT">
            </property>
            <property name="suppcmobile" type="integer" column="SUPPCMOBILE">
            </property>
            <property name="suppcunicom" type="integer" column="SUPPCUNICOM">
            </property>
            <property name="suppctelecom" type="integer" column="SUPPCTELECOM">
            </property>
            <property name="warnlimit" type="integer" column="WARNLIMIT">
            </property>
            <property name="warnmobile" type="java.lang.String" column="WARNMOBILE">
            </property>

    </class>
</hibernate-mapping>
