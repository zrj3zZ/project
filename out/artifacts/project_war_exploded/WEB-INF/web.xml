<?xml version="1.0" encoding="UTF-8"?>
<web-app id="hibpm_root" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>pm</display-name>	
  	<context-param>
      <param-name>webAppRootKey</param-name>
      <param-value>pm.root</param-value>
    <!-- Spring Config -->
    </context-param>
    
	<context-param>
		<param-name>log4jConfigLocation</param-name>
		<param-value>/WEB-INF/classes/log4j.properties</param-value>
	</context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>	
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value> 
		    /WEB-INF/applicationContext.xml,/WEB-INF/cxfbeans.xml,
            /WEB-INF/applicationContext-*.xml
        </param-value>
    </context-param> 
    <!-- 监听类  --> 
     <listener>   
        <listener-class>org.springframework.web.util.IntrospectorCleanupListener</listener-class>   
    </listener>
	 <!-- 会话监听类  --> 
	 <listener>   
        <listener-class>com.iwork.core.session.SessionListener</listener-class>   
    </listener>
   
  	 <listener>
		<listener-class>org.apache.struts2.tiles.StrutsTilesListener</listener-class>
	</listener>
	 <listener>
    	<listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
  	</listener>
	<filter>
		<filter-name>CharacterEncodingFilter</filter-name>
		<filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>utf-8</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>CharacterEncodingFilter</filter-name>
		<url-pattern>*.action</url-pattern>
	</filter-mapping>
  	<!-- 解决xss漏洞 -->  
  	<filter>  
	    <filter-name>xssFilter</filter-name>  
	    <filter-class>com.iwork.core.filter.XssFilter</filter-class>  
	</filter>  
	<filter-mapping>  
	    <filter-name>xssFilter</filter-name>  
	    <url-pattern>*.action</url-pattern>  
	</filter-mapping>
  	<filter>
        <filter-name>CacheFilter</filter-name>
        <filter-class>com.opensymphony.oscache.web.filter.CacheFilter</filter-class>
    </filter>
    <filter>
        <filter-name>CacheFilter-disableCacheOnMethods</filter-name>
        <filter-class>com.opensymphony.oscache.web.filter.CacheFilter</filter-class>
        <init-param>
            <param-name>time</param-name>
            <param-value>60</param-value>
        </init-param>
        <init-param>
            <param-name>disableCacheOnMethods</param-name>
            <param-value>POST,PUT,DELETE</param-value>
        </init-param>
        <init-param>
            <param-name>oscache-properties-file</param-name>
            <param-value>/oscache-cachefilter-disableCacheOnMethods.properties</param-value>
        </init-param>
    </filter>
	<filter>   
        <filter-name>hibernateFilter</filter-name>   
        <filter-class>org.springframework.orm.hibernate3.support.OpenSessionInViewFilter</filter-class>   
    </filter>    
    <!-- Struts2 Config -->
    <filter>
        <filter-name>SessionFilter</filter-name>
        <filter-class>com.iwork.core.filter.SessionFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>SessionFilter</filter-name>
        <url-pattern>/dologin.action</url-pattern>
        <url-pattern>/login.action</url-pattern>
        <url-pattern>/getmobileyzm.action</url-pattern>
        <url-pattern>/login.jsp</url-pattern>
    </filter-mapping>
    <filter>  
    	<filter-name>struts2</filter-name>  
    	<filter-class>com.iwork.core.server.root.BaseFilterDispatcher</filter-class>
  	</filter> 
    <filter-mapping>
      <filter-name>struts2</filter-name>
      <url-pattern>*.action</url-pattern>
    </filter-mapping>
    <filter-mapping>
      <filter-name>struts2</filter-name>
      <url-pattern>*.jsp</url-pattern>
    </filter-mapping>   
